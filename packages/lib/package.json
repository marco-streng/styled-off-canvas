{
  "name": "styled-off-canvas",
  "version": "0.0.0-development",
  "description": "styled components off-canvas menu",
  "type": "module",
  "main": "dist/styled-off-canvas.cjs.js",
  "module": "dist/styled-off-canvas.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "lint": "eslint",
    "test": "vitest --coverage",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "build": "ts-node build.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marco-streng/styled-off-canvas.git"
  },
  "keywords": [
    "styled-components",
    "off-canvas",
    "navigation",
    "react",
    "menu",
    "mobile"
  ],
  "author": "Marco Streng",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marco-streng/styled-off-canvas/issues"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18",
    "styled-components": ">=6"
  },
  "homepage": "https://github.com/marco-streng/styled-off-canvas#readme",
  "devDependencies": {
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^22.12.0",
    "@types/react": "^19.0.6",
    "@types/react-dom": "^19.0.4",
    "@types/semantic-release": "^20.0.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "@vitest/coverage-v8": "^4.0.18",
    "esbuild": "^0.19.11",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "jsdom": "^23.0.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "styled-components": "^6.1.6",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.1",
    "typescript": "^5.3.3",
    "vitest": "^4.0.18"
  }
}
